', 'transmission','fuelType']\n","for col in cols:\n","  le = LabelEncoder()\n","  le.fit(pd.concat([train[col],test[col]],axis=0))\n","  train[col] = le.transform(train[col])\n","  test[col] = le.transform(test[col])\n","\n","#데이터 분할\n","from sklearn.model_selection import train_test_split\n","X_tr, X_val, y_tr, y_val = train_test_split(train,\n","                                            target,\n","                                            test_size=0.2,\n","                                            random_state=25)\n","# print(X_tr.shape, X_val.shape, y_tr.shape, y_val.shape)\n","\n","\n","#랜포회귀\n","from sklearn.ensemble import RandomForestRegressor\n","model = RandomForestRegressor()\n","model.fit(X_tr,y_tr)\n","pred = model.predict(X_val)\n","pred\n","\n","#성능 검증\n","from sklearn.metrics import root_mean_squared_error\n","rmse = root_mean_squared_error(y_val,pred)\n","print(rmse)\n","\n","print(target.mean())\n","# print(train.select_dtypes(include='object'))\n","\n","#test데이터 예측\n","real_pred = model.predict(test)\n","submit = pd.DataFrame({\n","                      'ID' : test_id,\n","                      'price' : real_pred\n","})\n","submit.to_csv('12342.csv',index=False)\n"],"metadata":{"id":"u8u7W8wllxlj","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1749980284406,"user_tz":-540,"elapsed":4086,"user":{"displayName":"정인승[PTKSL - JEONG IN SEONG] ‍","userId":"11777570631850391267"}},"outputId":"cb71b2a9-559d-4744-89b8-0e09e8f77b72"},"execution_count":97,"outputs":[{"output_type":"stream","name":"stdout","text":["2597.2566817261045\n","24725.67067087448\n"]}]},{"cell_type":"markdown","source":["작업 3유형"],"metadata":{"id":"xR249V_9mG0N"}},{"cell_type":"code","source":["import pandas as pd\n","df= pd.read_csv('https://raw.githubusercontent.com/Datamanim/datarepo/main/krdatacertificate/e5_p3_1.csv')\n","df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"1UVZEmovlxjU","exec