train = train.drop(['ID','Date','Weekday'],axis=1)\n","test = test.drop(['ID','Date','Weekday'],axis=1)\n","target = train.pop('Y')\n","train\n","\n","# #스켈링\n","train.select_dtypes(exclude='object')\n","from sklearn.preprocessing import RobustScaler\n","scaler= RobustScaler()\n","col = ['Humidity','Pressure']\n","train[col] = scaler.fit_transform(train[col])\n","test[col] = scaler.transform(test[col])\n","\n","#인코딩\n","train.select_dtypes(include='object').describe()\n","from sklearn.preprocessing import LabelEncoder\n","le = LabelEncoder()\n","cols = ['C1', 'C2', 'C3', 'C4']\n","for col in cols:\n","  le = LabelEncoder()\n","  train[col] = le.fit_transform(train[col])\n","  test[col] = le.transform(test[col])\n","\n","\n","#데이터 분할\n","from sklearn.model_selection import train_test_split\n","X_tr, X_val, y_tr, y_val = train_test_split(train,\n","                                            target,\n","                                            test_size=0.2,\n","                                            random_state=25)\n","print(X_tr.shape, X_val.shape, y_tr.shape, y_val.shape)\n","\n","\n","#랜포 -> 회귀\n","from sklearn.ensemble import RandomForestRegressor\n","model = RandomForestRegressor(random_state = 25, n_estimators = 300)\n","model.fit(X_tr, y_tr)\n","pred = model.predict(X_val)\n","pred\n","\n","#검증 mae\n","from sklearn.metrics import mean_absolute_error\n","mae = mean_absolute_error\n","mae(y_val, pred)\n","\n","\n","#최종 test\n","pred_final = model.predict(test)\n","\n","pred_submit = pd.DataFrame({\n","                              \"pred\" : pred_final})\n","\n","pred_submit.to_csv(\"1234231.csv\",index=False)\n","\n","#제출\n","\n","# n_estimators=500\n","# 11.185339263024142"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"PCvFIi37im1Y","executionInfo":{"status":"ok","timestamp":1749874104317,"user_tz":-540,"elapsed":9133,"user":{"displayName":"정인승[PTKSL - JEONG IN SEONG] ‍","userId":"11777570631850391267"}},"outputId":"43f29268-b586-4