.fit(pd.concat([X_train[col], X_test[col]], axis =0))\n","  X_train[col] = le.transform(X_train[col])\n","  X_test[col] = le.transform(X_test[col])\n","\n","\n","#데이터 분리\n","from sklearn.model_selection import train_test_split\n","X_tr, X_val, y_tr, y_val = train_test_split(X_train,\n","                                            target,\n","                                            test_size = 0.2,\n","                                            random_state = 2025)\n","\n","print(X_tr.shape, X_val.shape, y_tr.shape, y_val.shape)\n","\n","\n","\n","#랜포\n","from sklearn.ensemble import RandomForestRegressor\n","model = RandomForestRegressor()\n","model.fit(X_tr,y_tr)\n","pred = model.predict(X_val)\n","pred\n","\n","#모델 검증 및 평가\n","from sklearn.metrics import mean_absolute_error, mean_squared_error, root_mean_squared_error, mean_absolute_percentage_error, r2_score\n","mae = mean_absolute_error(y_val, pred)\n","mse = mean_squared_error(y_val, pred)\n","rmse = root_mean_squared_error(y_val, pred)\n","mape = mean_absolute_percentage_error(y_val, pred)\n","r2_score = r2_score(y_val, pred)\n","\n","print(f\"mae = {mae}\")\n","print(f\"mse = {mse}\")\n","print(f\"rmse = {rmse}\")\n","print(f\"mape = {mape}\")\n","print(f\"r2_score = {r2_score}\")\n","\n","\n","#기본\n","mae = 894.6201543235253\n","mse = 1889346.0699884424\n","rmse = 1374.5348558652277\n","mape = 0.07768978022774974\n","r2_score = 0.949119542109733\n","\n","\n","\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"P7cgsWveW1Ax","executionInfo":{"status":"ok","timestamp":1748498145658,"user_tz":-540,"elapsed":1756,"user":{"displayName":"정인승[PTKSL - JEONG IN SEONG] ‍","userId":"11777570631850391267"}},"outputId":"997dba0b-6f66-4c5e-926c-c6938178e75c"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["(3110, 8) (778, 8) (3110,) (778,)\n","mae = 894.6201543235253\n","mse = 1889346.0699884424\n","rmse = 1374.5348558652277\n","mape = 0.07768978022774974\n","